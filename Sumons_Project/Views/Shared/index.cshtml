    <script>
    $(document).ready(function () {
        moveElements();
            var url = window.location;
            // for sidebar menu entirely but not cover treeview
            $('ul.vertical-nav-menu a').filter(function () {
                return this.href != url;
            }).parent().removeClass('mm-show');

            // for sidebar menu entirely but not cover treeview
            $('ul.vertical-nav-menu a').filter(function () {
                return this.href == url;
            }).parent().addClass('mm-show');

            // for treeview
            $('ul.treeview-menu a').filter(function () {
                return this.href == url;
            }).parentsUntil(".vertical-nav-menu > .treeview-menu").addClass('mm-active');
            //$('[data-toggle="tooltip"]').tooltip();
            getPing();
        });


            function getPing () {
            var startTime = performance.now();
            $.ajax({
                url: '@Url.Action("Ping", "Home")',
                type: 'GET',
                success: function (data) {
                    var endTime = performance.now();
                    var pingTime = endTime - startTime;
                    var element = $('#Signal');
                    element.removeClass('good bad ok one-bar two-bars three-bars four-bars five-bars no');
                    if (pingTime < 30) {
                        element.addClass('good five-bars');
                    } else if (pingTime >= 30 && pingTime <= 60) {
                        element.addClass('good four-bars');
                    } else if (pingTime >= 61 && pingTime <= 100) {
                        element.addClass('ok three-bars');
                    } else if (pingTime >= 101 && pingTime <= 200) {
                        element.addClass('ok two-bars');
                    } else if (pingTime >= 201 && pingTime <= 500) {
                        element.addClass('bad one-bar');
                  }
                    else {
                        element.addClass('no');
                    }
                    $('#Ping').text(parseInt(pingTime) + ' ms');
                }
            });
        };


        function updateClock() {
            getPing();
        }

        setInterval(updateClock, 200000);//$(".dropdown-toggle").click(function () {
                                                                                                                                        //    $('[data-toggle="tooltip"], .tooltip').tooltip("hide");
                                                                                                                //});

    function moveElements() {
        if ($(window).width() > 992) {
            $('.PingContainer').children().appendTo('.header-dots');
        } else {
            $('.header-dots').children().appendTo('.PingContainer');
        }
    }

    $(window).resize(function () {
        moveElements();
    });
    </script>


